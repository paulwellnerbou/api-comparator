{
  "name": "api-comparator",
  "version": "1.0.0",
  "description": "CLI tool to compare API responses between reference and target environments",
  "type": "module",
  "bin": {
    "api-comparator": "./dist/index.js"
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --format esm",
    "dev": "bun run src/index.ts",
    "start": "bun run dist/index.js"
  },
  "keywords": ["api", "testing", "comparison", "restfox"],
  "author": "",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest"
  },
  "dependencies": {
    "diff": "^8.0.2",
    "eta": "^4.0.1",
    "fast-deep-equal": "^3.1.3"
  }
}
