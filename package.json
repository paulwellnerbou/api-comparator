{
  "name": "api-comparator",
  "version": "1.0.0-dev.0",
  "description": "CLI tool to compare API responses between reference and target environments",
  "repository": {
    "type": "git",
    "url": "https://github.com/paulwellnerbou/api-comparator"
  },
  "homepage": "https://github.com/paulwellnerbou/api-comparator",
  "type": "module",
  "bin": {
    "api-comparator": "./dist/index.js"
  },
  "scripts": {
    "build": "bun build src/index.ts --outdir dist --target node --format esm",
    "dev": "bun run src/index.ts",
    "start": "bun run dist/index.js",
    "release": "release-it",
    "release:minor": "release-it minor",
    "release:major": "release-it major"
  },
  "keywords": ["api", "testing", "comparison", "restfox"],
  "author": "Paul Wellner Bou <paul@wellnerbou.de>",
  "license": "MIT",
  "devDependencies": {
    "@types/bun": "latest",
    "release-it": "^19.0.6"
  },
  "dependencies": {
    "diff": "^8.0.2",
    "eta": "^4.0.1",
    "fast-deep-equal": "^3.1.3"
  }
}
